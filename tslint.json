{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "no-console": [
            false,
            "log",
            "error"
        ],
        "interface-over-type-literal": false,
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "interface-name": false
    }
}